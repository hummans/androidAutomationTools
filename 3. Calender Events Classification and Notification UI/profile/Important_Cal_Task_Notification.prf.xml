			<Str sr="arg0" ve="3">//var antitle = "Buy milk and it's important"
 var speak = true;
 var speak_more = false;
 var show_scene = false;

 //flash(global('Task_Counter'));

 if (antitle.match(/and do(n't| not) speak it */gi) != null) {
     speak = false;
     antitle = antitle.replace(/and do(n't| not) speak it */gi, "");
 }


 if (antitle.match(/ and it(s|'s| is) important */gi) != null) {
     show_scene = true;
     antitle = antitle.replace(/ and it(s|'s| is) important */gi, "");

 }

 if (antitle.match(/ and it(s|'s| is) very important */gi) != null) {
     show_scene = true;
     speak_more = true;
     antitle = antitle.replace(/ and it(s|'s| is) very important */gi, "");

 }

antitle = antitle.trim();


 switch (global('Task_Counter')) {
     case '1':
         setGlobal('Task_Text1', antitle);
         break;
     case '2':
         setGlobal('Task_Text2', antitle);
         break;
     case '3':
         setGlobal('Task_Text3', antitle);
         break;
     default:
         break;
 }


 if (show_scene) {

     var sn = "Important Task Short " + global('Task_Counter');
     var index = global('Task_Counter') - 1;
     var vertical_position = -60 + (index * 10);
     var ok = showScene(sn, "OverBlocking", -100, vertical_position, false, false);
    setGlobal('Task_Counter', ( global('Task_Counter') % 3) + 1);

 }

 if (speak) {
     say(antitle, 'default', 'default', 'Media', 5, 5, true, true);
 }

 if (speak_more) {

     showScene('Stop Imp Cal Task Speak', "OverBlocking", -95, 95, false, false);
     performTask('Speak Very Important Calender Task', priority, antitle);
 }</Str>
